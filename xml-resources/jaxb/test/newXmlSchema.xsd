<xs:schema version="1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           elementFormDefault="qualified">

    
   <xs:element name="Incidencias">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name ="Incidencia" maxOccurs="unbounded" minOccurs= "0">
          <xs:complexType>
            <xs:sequence>
        <xs:element type="xs:string" name="origen"/>
        <xs:element type="xs:string" name="destino"/>
        <xs:element type="xs:string" name="detalle"/>
        <xs:element type="xs:string" name="tipo"/>
      </xs:sequence>
      <xs:attribute type="xs:string" name="fechahora" />
    </xs:complexType>
        </xs:element>
   
  </xs:choice>
  </xs:complexType>
  </xs:element> 
    
    
    
    
</xs:schema>

